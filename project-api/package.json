  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.26",
    "@types/mongoose": "^5.5.0",
    "@types/node": "^10.12.12",
    "@types/swagger-ui-express": "^3.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
