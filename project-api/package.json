{
  "name": "project-api",
  "version": "1.0.0",
  "description": "This is the project-api service for my personal portfolio",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "cross-env PORT=8080 NODE_ENV=dev ts-node ./src/server.ts",
    "test": "cross-env NODE_ENV=test mocha --require ts-node/register test/**/*.spec.ts",
